{
  "name": "electrical-drawing-app",
  "version": "1.0.0",
  "description": "LLM-powered electrical drawing analysis application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend/services/file-processor && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend/services/file-processor && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend/services/file-processor && npm test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend/services/file-processor && npm run lint",
    "install:all": "npm install && cd frontend && npm install && cd ../backend/services/file-processor && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "workspaces": [
    "frontend",
    "backend/services/*"
  ],
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "socket.io": "^4.8.1"
  }
}
